pipeline {
    agent any
    stages {
        stage('Maven Project') {
            steps {
                git credentialsId: 'git', url: "git@github.com:vinoth4180/devops-class.git", branch: 'azula'
                sh "git checkout main"
                dir('MavenHelloWorldProject') {
                    sh "sudo apt install maven -y"
                }
                post {
                    success {
                        archiveArtifacts "MavenHelloWorldProject/target/*.jar"
                    }
                }
            }
        }
    }
}
